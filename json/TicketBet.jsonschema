            "id": "bifrost.BonusAmount",
            "properties": {
                "virtual": {
                    "type": "number"
                },
                "nonWinning": {
                    "type": "number"
                },
                "winning": {
                    "type": "number"
                },
                "total": {
                    "type": "number"
                },
                "free": {
                    "type": "number"
                },
