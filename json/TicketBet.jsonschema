                "$ref": "bifrost.TicketSelection",
                "additionalProperties": true,
                "type": "object"
            },
            "type": "array"
        },
        "won": {
            "oneOf": [
                {
                    "type": "null"
                },
                {
                    "type": "boolean"
                }
            ]
        },
        "voided": {
            "oneOf": [
                {
                    "type": "null"
                },
                {
                    "type": "boolean"
                }
            ]
        },
