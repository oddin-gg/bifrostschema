                "name": {
                    "type": "string"
                },
                "id": {
                    "type": "string"
                },
                "isHome": {
                    "oneOf": [
                        {
                            "type": "null"
                        },
                        {
                            "type": "boolean"
                        }
                    ]
                }
            },
            "additionalProperties": true,
            "type": "object"
        },
        "bifrost.TicketEvent": {
            "id": "bifrost.TicketEvent",
            "properties": {
                "name": {
                    "type": "string"
                },
                "id": {
                    "type": "string"
                },
                "sportId": {
                    "type": "string"
