                    "oneOf": [
                        {
                            "type": "null"
                        },
                        {
                            "type": "boolean"
                        }
                    ]
                }
            },
            "additionalProperties": true,
            "type": "object"
        },
        "bifrost.TicketSelection": {
            "id": "bifrost.TicketSelection",
