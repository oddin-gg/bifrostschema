                        "name": {
                            "type": "string"
                        },
                        "id": {
                            "type": "string"
                        },
                        "marketId": {
                            "type": "string"
                        },
                        "marketName": {
                            "type": "string"
                        },
                        "outcomeId": {
                            "type": "string"
                        }
                    },
                    "additionalProperties": true,
                    "type": "object"
                },
                "odds": {
                    "type": "number"
                },
                "event": {
                    "$ref": "bifrost.TicketEvent",
                    "additionalProperties": true,
                    "type": "object"
                },
                "voided": {
                    "oneOf": [
                        {
                            "type": "null"
                        },
                        {
                            "type": "boolean"
                        }
                    ]
                },
                "won": {
                    "oneOf": [
                        {
                            "type": "null"
                        },
                        {
                            "type": "boolean"
                        }
                    ]
                }
            },
            "additionalProperties": true,
            "type": "object"
        },
